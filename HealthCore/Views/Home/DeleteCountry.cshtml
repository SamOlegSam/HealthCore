@model HealthCore.Models.Country

<div class="modal-header" style="text-align: center; background-color: #d4d7d9; border-radius:5px;">
    @*<button class="close" data-dismiss="modal" area-hidden="true"><i class="fa fa-close"></i></button>*@
    <h5 style="text-align:center"><b>Удаление</b></h5>
</div>
<div class="modal-body" style="text-align: center">


    <h5>Вы точно хотите удалить страну"@Model.Name"?</h5>

</div>
<div class="modal-footer" style="text-align: center">
    <button type="submit" class="btn btn-outline-danger" style="min-width:100px" onclick="DeleteCountryOK(@Model.Id)">Да</button>
    <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal" style="min-width:100px">Нет</button>
</div>





